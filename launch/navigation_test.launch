<?xml version="1.0"?>
<launch>

  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find navigation_test)/worlds/benchmark.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/> 
    <arg name="recording" value="false"/>
    <arg name="debug" value="false"/>
  </include>

  <include file="$(find navigation_test)/launch/one_robot.launch" >
        <arg name="ns" value="p3dx_0" />
        <arg name="x"  value="0.0" />
        <arg name="y"  value="0.0" />
        <arg name="z"  value="0.0" />
  </include>

  <include file="$(find navigation_test)/launch/one_robot.launch" >
        <arg name="ns" value="p3dx_1" />
        <arg name="x"  value="0.0" />
        <arg name="y"  value="5.0" />
        <arg name="z"  value="0.0" />
  </include>

  <include file="$(find navigation_test)/launch/one_robot.launch" >
        <arg name="ns" value="p3dx_2" />
        <arg name="x"  value="0.0" />
        <arg name="y"  value="10.0" />
        <arg name="z"  value="0.0" />
  </include>

</launch>
